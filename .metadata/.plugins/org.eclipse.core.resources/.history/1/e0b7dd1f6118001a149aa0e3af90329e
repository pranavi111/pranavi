package com.interfaceimplementation;

public class Interface {

	class Vehicle
	{
		 public   void run()
		  {
			  System.out.println("Vehicle is running");
			  }
		  }
		  class Bike2 extends Vehicle
		  {
			  public   void run()
		  {
			  System.out.println("Bike is running safely");
			  }

		  public void main(String args[]){
		  Bike2 obj = new Bike2();
		  obj.run();
		  }
		}


}
