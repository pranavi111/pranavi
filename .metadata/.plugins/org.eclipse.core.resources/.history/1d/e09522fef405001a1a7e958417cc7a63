package collectionframework;

import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;

public class Hashmap {

	public static void main(String[] args) {
		Map<String,Integer> abc = new HashMap<String,Integer>();
		 abc.put("vvv",1);
		 abc.put("sss",2);
		 abc.put("ddd",3);
		 System.out.println(abc);    //op   {sss=2, ddd=3, vvv=1}
		 
		//System.out.println(abc.get(1));
	

		for (Entry<String, Integer> data : abc.entrySet()) {

			

			System.out.print(data.getValue());//231
			System.out.println(abc.containsKey(vvvv));//op:2false
			                                            // 3false
			                                             //1false


		}

		//System.out.println(abc.get(3));

	}

}
